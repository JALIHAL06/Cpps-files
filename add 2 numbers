#include <stdio.h>
int  inputaddnums(int *a,int *b)
{
      printf("enter two numbers");
      scanf("%d",&*a);
      scanf("%d",&*b);
}
int addnums(int x,int y) 
{
      int c;
      c=x+y;
      return c;
}
int output(int c)
{
    printf("sum=%d",c);
}
void main()
{
    int f,g,h;
    inputaddnums(&f,&g);
    h=addnums(f,g);
    output(h);
}
